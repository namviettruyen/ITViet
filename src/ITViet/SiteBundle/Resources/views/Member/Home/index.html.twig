{% extends 'ITVietSiteBundle::layout.html.twig' %}

{% block navigator %}
  {% include 'ITVietSiteBundle::navigator.html.twig' with {'active':'profile'} %}
{% endblock %}

{% block title %}{% trans %}Profile{% endtrans %}{% endblock %}

{% block maincontents %}

<h4 class="full-name">{{ member.fullName }}</h1>

<div class="profile group">
  <div class="gravatar">
    <a href="#">
      <img class="img-polaroid" src="/images/1.png" width="128" height="128" alt="" />
    </a>
    <div>432EXP</div>
  </div>
  <div class="data">
    <table>
      <tr>
        <td width="30%">{% trans %}From{% endtrans %}:</td>
        <td>{{ member.address }}</td>
      </tr>
      <tr>
        <td>{% trans %}Register{% endtrans %}:</td>
        <td>{{ member.since|date('d-m-Y') }}</td>
      </tr>
      <tr>
        <td>{% trans %}Gender{% endtrans %}:</td>
        <td>{{ member.gender }}</td>
      </tr>
      <tr>
        <td>{% trans %}Age{% endtrans %}:</td>
        <td>{{ member.birthDate|date('d-m-Y') }}</td>
      </tr>
    </table>
  </div>
</div>

<ul id="myTab" class="nav nav-tabs">
  <li class="active"><a href="#home" data-toggle="tab">Home</a></li>
  <li><a href="#profile" data-toggle="tab">Profile</a></li>
  <li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">{% trans %}Manager Account{% endtrans %} <b class="caret"></b></a>
    <ul class="dropdown-menu">
      <li><a href="#dropdown1" data-toggle="tab">{% trans %}Change information{% endtrans %}</a></li>
      <li><a href="#dropdown2" data-toggle="tab">{% trans %}Change email{% endtrans %}</a></li>
      <li><a href="#dropdown3" data-toggle="tab">{% trans %}Change password{% endtrans %}</a></li>
    </ul>
  </li>
</ul>

