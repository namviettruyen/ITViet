{% set jsParams = toJsObj(paginator.params) %}

<div class="pagination pagination-right">
  <ul>
    {% if paginator.isFirst %}
    <li class="disabled">
      <a>&laquo;</a>
    </li>
    {% else %}
    <li><a onclick="jumpToPage($.extend({{ jsParams }}, {'page':{{ paginator.page-1 }}})); return false;" href="#">&laquo;</a></li>
    {% endif %}

    {% for blck in paginator.pageBlocks %}
      {% for i in range(blck[0], blck[1]) %}
        {% if paginator.page == i %}
          <li class="active"><a href="#">{{ i }}</a></li>
        {% else %}
          <li><a onclick="jumpToPage($.extend({{ jsParams }}, {'page':{{ i }}})); return false;" href="#">{{i}}</a></li>
        {% endif %}
      {% endfor %}
      {% if not loop.last %}
        <li><a>...</a></li>
      {% endif %}
    {% endfor %}

    {% if paginator.isLast %}
    <li class="disabled">
      <a>&raquo;</a>
    </li>
    {% else %}
    <li><a onclick="jumpToPage($.extend({{ jsParams }}, {'page':{{ paginator.page+1 }}})); return false;"  href="#">&raquo;</a></li>
    {% endif %}
  </ul>
</div>
